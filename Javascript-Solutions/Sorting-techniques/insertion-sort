// Insertion Sort

class Solution
{
  insert(arr,i){
     // code here such that insertionSort() sorts arr[]
  }

   //Function to sort the array using insertion sort algorithm.
  insertionSort(arr,n){
   //code here
   for(let i=1;i<n;i++){
      let j=i-1;
       let val=arr[i]
       
       while(j>-1 && arr[j]>val){
           arr[j+1]=arr[j]
           j--
       }
       arr[j+1]=val
   }
  }
    
}
