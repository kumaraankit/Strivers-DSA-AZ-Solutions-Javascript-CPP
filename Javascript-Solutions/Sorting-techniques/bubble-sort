// Bubble- Sort


class Solution
{
   //Function to sort the array using bubble sort algorithm.
   bubbleSort(arr,n){
         let count=1;
        while(n>count){
            for(let i=0;i<n-count;i++){
                if(arr[i]>arr[i+1]){
                    [arr[i],arr[i+1]]=[arr[i+1],arr[i]]
                }
            }
               count++;
        }
     
    //code here
   }
    
}
